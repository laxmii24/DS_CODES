#include <stdio.h>

int graph[20][20], visited[20], stack[20];
int n, top = -1;

void topoDFS(int v) {
    visited[v] = 1;

    for (int i = 1; i <= n; i++) {
        if (graph[v][i] == 1 && !visited[i])
            topoDFS(i);
    }

    stack[++top] = v;
}

int main() {
    int edges, u, v;

    printf("Enter number of vertices: ");
    scanf("%d", &n);

    printf("Enter number of edges: ");
    scanf("%d", &edges);

   
    for (int i = 1; i <= n; i++)
        for (int j = 1; j <= n; j++)
            graph[i][j] = 0;

    printf("Enter directed edges (u v):\n");
    for (int i = 0; i < edges; i++) {
        scanf("%d %d", &u, &v);
        graph[u][v] = 1;
    }

    for (int i = 1; i <= n; i++)
        visited[i] = 0;

  
    for (int i = 1; i <= n; i++) {
        if (!visited[i])
            topoDFS(i);
    }

    printf("Topological Order: ");
    while (top != -1)
        printf("%d ", stack[top--]);

    return 0;
}
