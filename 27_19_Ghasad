#include <stdio.h>
#include <string.h>

int graph[20][20], visited[20], low[20], disc[20], parent[20];
int n, timeD;
int isArticulation = 0;

void DFS(int u) {
    visited[u] = 1;
    disc[u] = low[u] = ++timeD;
    int child = 0;

    for (int v = 1; v <= n; v++) {
        if (graph[u][v]) {
            if (!visited[v]) {
                child++;
                parent[v] = u;
                DFS(v);

                low[u] = (low[u] < low[v]) ? low[u] : low[v];

                if (parent[u] == -1 && child > 1)
                    isArticulation = 1;

                if (parent[u] != -1 && low[v] >= disc[u])
                    isArticulation = 1;

            } else if (v != parent[u]) {
                low[u] = (low[u] < disc[v]) ? low[u] : disc[v];
            }
        }
    }
}

int main() {
    int edges, u, v;

    printf("Enter number of vertices: ");
    scanf("%d", &n);

    memset(graph, 0, sizeof(graph));
    memset(visited, 0, sizeof(visited));
    memset(parent, -1, sizeof(parent));

    printf("Enter number of edges: ");
    scanf("%d", &edges);

    printf("Enter edges (u v):\n");
    for (int i = 0; i < edges; i++) {
        scanf("%d %d", &u, &v);
        graph[u][v] = graph[v][u] = 1;
    }

    DFS(1);

    for (int i = 1; i <= n; i++) {
        if (!visited[i]) {
            printf("Graph is NOT Biconnected (Not Connected)\n");
            return 0;
        }
    }

    if (isArticulation)
        printf("Graph is NOT Biconnected (Articulation Point Exists)\n");
    else
        printf("Graph IS Biconnected\n");

    return 0;
}
